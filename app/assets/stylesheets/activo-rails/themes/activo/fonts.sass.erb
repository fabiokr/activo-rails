// Web fonts

// From http://www.goodwebfonts.com/museo_sans_500/index.html

// A font by Jos Buivenga (http://www.exljbris.com)
@font-face
  font-family: 'MuseoSans500'
  src: url('<%= asset_path 'activo-rails/museosans_500-webfont.eot' %>')
  src: local("☺"), url('<%= asset_path 'activo-rails/museosans_500-webfont.woff' %>') format("woff"), url('<%= asset_path 'activo-rails/museosans_500-webfont.ttf' %>') format("truetype"), url('<%= asset_path 'activo-rails/museosans_500-webfont.svg' %>#webfontxgyHgdAL') format("svg")
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'MuseoSans500Italic'
  src: url('<%= asset_path 'activo-rails/museosans_500_italic-webfont.eot' %>')
  src: local("☺"), url('<%= asset_path 'activo-rails/museosans_500_italic-webfont.woff' %>') format("woff"), url('<%= asset_path 'activo-rails/museosans_500_italic-webfont.ttf' %>') format("truetype"), url('<%= asset_path 'activo-rails/museosans_500_italic-webfont.svg' %>#webfonteCRZCYEf') format("svg")
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Museo700'
  src: url('<%= asset_path 'activo-rails/museo700-regular-webfont.eot' %>')
  src: local("☺"), url('<%= asset_path 'activo-rails/museo700-regular-webfont.woff' %>') format("woff"), url('<%= asset_path 'activo-rails/museo700-regular-webfont.ttf' %>') format("truetype"), url('<%= asset_path 'activo-rails/museo700-regular-webfont.svg' %>#webfontOC5Ke5xr') format("svg")
  font-weight: normal
  font-style: normal
